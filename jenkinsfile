@Library('mylibrary')_
pipeline
{
  agent any
  stages
  {
    stage('cont download_master')
    {
      steps
      {
        script
	{
	  venki.gitDownload("maven")
	}
      }
    }
    stage('cont build_master')
    {
      steps
      {
        script
	{
	  venki.gitBuild()
	}
      }
    }
  }
}
